<view><view class="registered_account"><view class="registered_account_phone"><input type="number" maxlength="11" placeholder="请输入手机号码" placeholder-class="placeholder_style" data-event-opts="{{[['blur',[['checkPhone',['$event']]]],['input',[['__set_model',['','phoneNum','$event',[]]]]]]}}" value="{{phoneNum}}" bindblur="__e" bindinput="__e"/><block wx:if="{{!beginFalg}}"><view data-event-opts="{{[['tap',[['sendCode',['$event']]]]]}}" class="registered_account_send" bindtap="__e">{{sendCodeText}}</view></block><block wx:if="{{beginFalg}}"><view class="{{['registered_account_send',beginFalg?' beginClass':'']}}">{{timeText}}</view></block></view><view class="registered_account_code"><input type="number" placeholder="请输入短信验证码" placeholder-class="placeholder_style" data-event-opts="{{[['blur',[['checkCode',['$event']]]],['input',[['__set_model',['','smsCode','$event',[]]]]]]}}" value="{{smsCode}}" bindblur="__e" bindinput="__e"/></view><view class="registered_account_new"><input style="{{'font-size:'+(system=='IOS'?'8px;':'20px')+';'}}" type="text" password="true" maxlength="15" placeholder="请输入新密码(6-15位数字,字母组合)" placeholder-class="placeholder_style" data-event-opts="{{[['blur',[['checkPwd',['$event']]]],['input',[['__set_model',['','password','$event',[]]]]]]}}" value="{{password}}" bindblur="__e" bindinput="__e"/></view><view class="registered_account_agreement"><checkbox value="" checked="{{checkFalg}}" data-event-opts="{{[['tap',[['isAgree',['$event']]]]]}}" bindtap="__e"></checkbox><view data-event-opts="{{[['tap',[['toRule',['$event']]]]]}}" class="agreement_content" bindtap="__e">请阅读并同意《赚分享用户使用协议》及《赚分享用户服务协议》</view></view><view data-event-opts="{{[['tap',[['register',['$event']]]]]}}" class="registered_account_btn" bindtap="__e">立即注册</view></view></view>